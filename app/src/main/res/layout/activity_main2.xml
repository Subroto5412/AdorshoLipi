<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/soroborno_bg"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/left_side"
        android:layout_width="@dimen/_55sdp"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/back_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_15sdp"
            android:src="@drawable/back"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/home_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/back_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:src="@drawable/home"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/sound_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/home_btn"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="@dimen/_10sdp"
            android:src="@drawable/music"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        app:layout_constraintStart_toEndOf="@id/left_side"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="@dimen/_13sdp"
        android:layout_marginTop="@dimen/_13sdp"
        android:layout_marginBottom="@dimen/_13sdp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layer01"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_82sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_1"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:src="@drawable/soreo"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_2"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_1"
                android:src="@drawable/soreya"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_3"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_2"
                android:src="@drawable/hoshoi"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_4"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_3"
                android:src="@drawable/digohi"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/layer02"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_82sdp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/layer01">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/soroborno_5"
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_66sdp"
                    android:src="@drawable/hosou"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ImageContrastCheck" />

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/soroborno_6"
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/soroborno_5"
                    android:src="@drawable/hosohi"/>

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/soroborno_7"
                    android:layout_width="@dimen/_130sdp"
                    android:layout_height="match_parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toEndOf="@id/soroborno_6"
                    android:src="@drawable/rie"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/layer03"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_82sdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/layer02">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_8"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:src="@drawable/e"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_9"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_8"
                android:src="@drawable/oi"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_10"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_9"
                android:src="@drawable/oo"/>

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/soroborno_11"
                android:layout_width="@dimen/_130sdp"
                android:layout_height="match_parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@id/soroborno_10"
                android:src="@drawable/oui"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>